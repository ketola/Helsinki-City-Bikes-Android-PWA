{
  "name": "Helsinki-City-Bikes-PWA",
  "version": "1.0.0",
  "description": "Progressive Web App implementation of the Helsinki City Bikes Android App",
  "main": "index.js",
  "author": "Sauli Ketola",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist js && mkdir js dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress && cp -r index.html manifest.json sw.js js public dist && tar -zcvf dist.tar.gz dist/ ",
    "prod:s3": "aws s3 sync dist s3://helsinki-city-bikes --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --profile citybikes",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "geolib": "^2.0.22",
    "geolocator": "^2.1.1",
    "google-map-react": "^0.24.0",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router": "2.8.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.43.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.4",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "serviceworker-webpack-plugin": "^0.2.1",
    "sw-precache": "^5.2.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
